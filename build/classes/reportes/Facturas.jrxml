	<queryString>
		<![CDATA[SELECT 
    f.id AS idFactura, 
    f.numero_factura,
    f.fecha_emision, 
    f.subtotal, 
    f.descuento, 
    f.total, 
    f.metodo_pago,
    f.nombre_cliente AS nombreCliente,
    COALESCE(f.correo_electronico, '') AS correoCliente,
    f.direccion_facturacion AS direccionCliente,
    f.numero_documento AS ccCliente,
    f.cufe AS cufeFactura,
    COALESCE(u.nombre_completo, v.usuario) AS nombreVendedor,
    v.id AS idVenta,
    dv.codigo_producto,
    dv.nombre_producto,
    dv.cantidad,
    dv.precio_unitario,
    COALESCE(dv.iva, 0) AS iva,
    dv.subtotal AS totalDetalle
FROM factura f
INNER JOIN ventas v ON v.id = f.venta_id
INNER JOIN detalle_ventas dv ON dv.venta_id = v.id
LEFT JOIN usuarios u ON u.nombre_usuario = v.usuario
WHERE f.id = $P{idFactura}]]>
	</queryString>
